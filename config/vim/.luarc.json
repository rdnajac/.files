{
  "runtime": {
    "version": "LuaJIT",
    "path": ["lua/?.lua", "lua/?/init.lua"]
  },
  "diagnostics": {
    "globals": ["vim", "Snacks"],
    "disable": ["missing-fields"]
  },
  "workspace": {
    "checkThirdParty": false,
    "library": [
      "$VIMRUNTIME",
      "${3rd}/luv/library",
      "./lua",
      "./.plugged/blink.cmp/lua",
      "./.plugged/nvim-treesitter/lua",
      "./.plugged/snacks.nvim/",
      "./.plugged/which-key.nvim/lua"
    ]
  }
}

